{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "es5"],
        "module": "commonjs",
        "types": ["cypress", "node"],
        "noEmit": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "include": [
        "cypress/**/*.ts", 
        "cypress.config.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
  
